{
    "version": "0.2.0",
    "configurations": [
        {
        "name": "Video4Linux debug",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/build/${config:VARISCITE.APPDIR_V4L}",
        "args": [],
        "stopAtEntry": true,
        "cwd": "${workspaceFolder}/${config:VARISCITE.APPDIR_V4L}",
        "environment": [],
        "console": "integratedTerminal",
        "MIMode": "gdb",
        "targetArchitecture": "arm64",
        "preLaunchTask": "var-deploy-gdb_v4l",
        "setupCommands": [{
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
        }],
        "miDebuggerPath": "/usr/bin/gdb-multiarch",
