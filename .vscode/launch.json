{
    "version": "0.2.0",
    "configurations": [{
        "name": "GStreamer debug",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/build/${config:VARISCITE.APPDIR_GST}",
        "args": [],
        "stopAtEntry": true,
        "cwd": "${workspaceFolder}/${config:VARISCITE.APPDIR_GST}",
        "environment": [],
        "console": "integratedTerminal",
        "MIMode": "gdb",
        "targetArchitecture": "arm64",
        "preLaunchTask": "var-deploy-gdb_gst",
        "setupCommands": [{
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
        }],
        "miDebuggerPath": "/usr/bin/gdb-multiarch",
        "miDebuggerServerAddress": " ${config:VARISCITE.TARGET_IP}:3000",
    }
]
}